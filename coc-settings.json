{
  "inlayHint.position": "eol",
  "diagnostic.errorSign": ">>",
  "diagnostic.warningSign": "--",
  "diagnostic.infoSign": "..",
  "diagnostic.hintSign": "..",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "rust",
    "vue",
    "svelte",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "tf"
  ],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    },
    "ocaml": {
      "command": "ocamllsp",
      "args": [],
      "filetypes": ["ocaml", "reason"],
      "initializationOptions": {},
      "settings": {}
    },
    "lean": {
      "command": "lean",
      "args": ["--server"],
      "filetypes": ["lean"],
      "initializationOptions": {},
      "settings": {}
    }
    // "protobuf": {
    //   "command": "protocol-buffers-language-server",
    //   "args": ["--logfile=/tmp/prbuf-lsp.log", "--loglevel=debug"],
    //   "filetypes": ["proto"]
    // }
  },
  "diagnostic-languageserver.linters": {
    "hadolint": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hadolint/hadolint",
        "hadolint",
        "-f",
        "json",
        "-t",
        "style",
        "-"
      ],
      "sourceName": "hadolint",
      "parseJson": {
        "line": "line",
        "column": "column",
        "security": "level",
        "message": "${message} [${code}]"
      },
      "securities": {
        "error": "error",
        "warning": "warning",
        "info": "info",
        "style": "hint"
      }
    },
    "shellcheck": {
      "command": "shellcheck",
      "debounce": 100,
      "args": [
        "--format",
        "json",
        "-x",
        "-"
      ],
      "sourceName": "shellcheck",
      "parseJson": {
        "line": "line",
        "column": "column",
        "endLine": "endLine",
        "endColumn": "endColumn",
        "message": "${message} [${code}]",
        "security": "level"
      },
      "securities": {
        "error": "error",
        "warning": "warning",
        "info": "info",
        "style": "hint"
      }
    }
  },
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck",
    "dockerfile": "hadolint"
  },
  "coc.source.neosnippet.priority": 40,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "tsserver.formatOnType": true,
  "tsserver.useLocalTsdk": true,
  "eslint.format.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.rules.customizations": [
    {
       "rule": "prettier/*",
       "severity": "off"
    },
    // {
    //    "rule": "@typescript-eslint/prefer-readonly-parameter-types",
    //    "severity": "off"
    // },
    // {
    //    "rule": "max-lines-per-function",
    //    "severity": "off"
    // },
    // {
    //    "rule": "max-statements",
    //    "severity": "off"
    // },
  ],
  "emmet.priority": 30,
  "deno.codeLens.references": false,
  "deno.codeLens.referencesAllFunctions": false,
  "deno.codeLens.implementations": false,
  "deno.cacheOnSave": true,
  "codeLens.enable": false,
  "go.goplsOptions": {
    "staticcheck": true
  },
  "coc.preferences.enableMessageDialog": false,
  "prettier.onlyUseLocalVersion": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.experimental.procAttrMacros": true,
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.server.extraEnv": {
      "CARGO_TARGET_DIR": "target/analyzer"
  },
  "rust-analyzer.check.extraArgs": [
      "--target-dir=target/analyzer"
  ],
  "svelte.enable-ts-plugin": true,
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.disableProgressNotifications": true,
  "rust-analyzer.checkOnSave.noDefaultFeatures": null
}
